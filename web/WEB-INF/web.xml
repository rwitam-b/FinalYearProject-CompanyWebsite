<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ClientCompanyLogin</servlet-name>
        <servlet-class>ClientCompanyLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientLogout</servlet-name>
        <servlet-class>ClientLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CryptexEntry</servlet-name>
        <servlet-class>CryptexEntry</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientCompanyLogin</servlet-name>
        <url-pattern>/ClientCompanyLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientLogout</servlet-name>
        <url-pattern>/ClientCryptex/ClientLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CryptexEntry</servlet-name>
        <url-pattern>/ClientCryptex/CryptexEntry</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
